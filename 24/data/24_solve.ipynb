    "with open(\"24-prob.txt\", 'r') as f:\n",
    "    data = list(f.read())\n",
    "\n",
    "cnt = 0\n",
    "\n",
    "for i in range(0, len(data), 2):\n",
    "    c1 = data[i] == \"A\" and data[i + 1] == \"B\"\n",
    "    c2 = data[i] == \"C\" and data[i + 1] == \"B\"\n",
    "    if c1 or c2:\n",
    "        cnt += 2\n",
    "\n",
    "print(cnt)"
   ]
  }
